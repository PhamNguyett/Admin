        <div class="product-status mg-b-15">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="product-status-wrap">
                            <h4>Product List</h4>
                            <div class="add-product">
                                <a href="/product/add">Add Product</a>
                            </div>
                            <div class="asset-inner">
                                <table>
                                    <tr>
                                        <th>No</th>
                                        <th>Image</th>
                                        <th>Name of product</th>
                                        <th>Status</th>
                                        <th>Color</th>
                                        <th>Slug</th>
                                        <th style="color: rgb(250, 39, 39);">Promotional price</th>
                                        <th>Price</th>
                                        <th>Setting</th>
                                    </tr>
                                    {{#each allProduct}}
                                    <tr>
                                        <td>1</td>
                                        <td><img src={{imagesUrl}} alt="" /></td>
                                        <td>{{name}}</td>
                                        <td>
                                            <button class="pd-setting">Active</button>
                                        </td>
                                        <td>Plaza</td>
                                        <td>{{slug}}</td>
                                        <td>{{pricePromotion}}</td>
                                        <td>{{price}}$</td>
                                        <td>
                                            <a href= "/product/edit"><button data-toggle="tooltip" title="Edit" class="pd-setting-ed"><i class="fa fa-pencil-square-o" ></i></button></a>
                                            <button data-toggle="tooltip" title="Trash" class="pd-setting-ed"><i class="fa fa-trash-o" aria-hidden="true"></i></button>
                                        </td>
                                    </tr>
                                    {{/each}}
                                    
                                </table>
                            </div>
                            <div class="custom-pagination">
								<ul class="pagination">
									<li class="page-item"><a class="page-link" href="#">Previous</a></li>
									<li class="page-item"><a class="page-link" href="#">1</a></li>
									<li class="page-item"><a class="page-link" href="#">2</a></li>
									<li class="page-item"><a class="page-link" href="#">3</a></li>
									<li class="page-item"><a class="page-link" href="#">Next</a></li>
								</ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>