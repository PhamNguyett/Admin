

<div class="breadcome-area">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="breadcome-list">
                    <div class="row">
                        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
                            <div class="breadcome-heading" style="display: flex;">
                                <li class="nav-item" style="list-style-type: none; padding: 12px;font-size: 16px;"><a href="/order" class="nav-link">All orders</a>
                                </li>
                                {{#each allOrder}}
                                {{#if status}}
                                <li class="nav-item" style="list-style-type: none; padding: 12px;font-size: 16px;"><a href="/order/{{_id}}" class="nav-link">{{_id}}</a>
                                </li>
                                {{/if}}
                                {{/each}}
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                            <ul class="breadcome-menu">
                                <li><a href="/order" style="color: rgb(59, 33, 231);">Order</a> <span class="bread-slash">/</span>
                                </li>
                                <li><span class="bread-blod">View</span>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

